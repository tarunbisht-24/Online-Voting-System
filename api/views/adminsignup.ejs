	<script>
		
		function check(){
			var secretPassword= document.getElementById("secretPassword");    
			var trueSecretPassword = "715364924";
			if(secretPassword.value != trueSecretPassword){
				alert("Invalid Secret Password!\nPlease Contact +919872779838 for Admin Authorizaion");
				return false;
			}
		}
		window.onload = function() {
			const url = new URL(window.location.href);
			const params= url.searchParams;
			const Message = params.get('message');
			document.getElementById("label").innerHTML = Message;
			var date = new Date();
			var dd = date.getDate();
			var mm = date.getMonth();
			var yyyy = date.getFullYear() - 18;
			if(dd<10){
				dSlash = "-0";
			} else {
				dSlash = "-";
			}
			if(mm<10){
				mSlash = "-0";
			} else {
				mSlash = "-";
			}
			document.getElementById("dob").max = yyyy+mSlash+mm+dSlash+dd;
			document.getElementById("dob").value = yyyy+mSlash+mm+dSlash+dd;
		}
	</script>
